<%- include('partials/header') %>
<%- include('partials/navbar') %>

<!-- ====================================
         HERO SECTION - PREMIUM CLEAN DESIGN
         ==================================== -->
<section id="home" class="hero-section-logistiqo">
  <!-- White Page Background -->
  <div class="hero-page-bg"></div>
  
  
  <!-- Professional Edge Frame -->
  <div class="hero-edge-frame">
    <!-- Blue Accent Strokes - All 4 Corners -->
    <div class="corner-stroke corner-tl"></div>
    <div class="corner-stroke corner-tr"></div>
    <div class="corner-stroke corner-bl"></div>
    <div class="corner-stroke corner-br"></div>
    <!-- Outer Edge -->
    <div class="edge-outer"></div>
    <!-- Inner Edge -->
    <div class="edge-inner"></div>
    <!-- Floating Particles -->
    <div class="hero-particles">
      <span class="particle p1"></span>
      <span class="particle p2"></span>
      <span class="particle p3"></span>
      <span class="particle p4"></span>
      <span class="particle p5"></span>
    </div>
    <!-- Inset Image/Video Container -->
    <div class="hero-inset-image">
      <!-- Background Video (hidden initially) -->
      <video class="hero-bg-video" id="heroVideo" muted loop playsinline poster="https://images.unsplash.com/photo-1503376780353-7e6692767b70?q=80&w=2070&auto=format&fit=crop">
        <source src="https://videos.pexels.com/video-files/3130284/3130284-uhd_2560_1440_30fps.mp4" type="video/mp4">
      </video>
      <!-- Poster Image (visible initially) -->
      <img src="https://images.unsplash.com/photo-1503376780353-7e6692767b70?q=80&w=2070&auto=format&fit=crop" alt="Luxury Sports Car" class="hero-bg-image" id="heroPoster">
      <div class="hero-image-overlay"></div>
      
      <!-- Play Video Button -->
      <button class="hero-play-btn" id="heroPlayBtn" aria-label="Play background video">
        <div class="play-btn-circle">
          <i data-lucide="play"></i>
        </div>
        <span class="play-btn-text">Watch Video</span>
      </button>
    </div>
  </div>
  
  <!-- Main Content Container -->
  <div class="container hero-logistiqo-container">
    <div class="hero-content-wrapper">
      <!-- Left Content - Text with Staggered Animations -->
      <div class="hero-text-content">
        <!-- Badge -->
        <div class="hero-badge animate-slide-left" style="--delay: 0.2s">
          <span class="badge-dot"></span>
          <span class="badge-label">Premium OEM & Aftermarket Parts</span>
        </div>
        
        <!-- Main Headline - Each Line Animated Separately -->
        <h1 class="hero-headline">
          <span class="headline-line animate-slide-left" style="--delay: 0.4s">WHERE GLOBAL SUPPLY BECOMES A</span>
          <span class="headline-line headline-gradient animate-slide-left" style="--delay: 0.6s">COMPETITIVE ADVANTAGE</span>
        </h1>

        <!-- Subheadline -->
        <p class="hero-subheadline animate-slide-left" style="--delay: 0.85s">
          We source OEM and aftermarket automotive components through a worldwide partner network. Backed by verified suppliers, competitive pricing, and seamless logistics.
        </p>

        <!-- CTA Button -->
        <div class="hero-cta-group animate-slide-left" style="--delay: 1.1s">
          <a href="#sectors" class="btn-cta-premium">
            <span class="btn-text">Let's Collaborate</span>
            <span class="btn-glow"></span>
          </a>
        </div>
      </div>
    </div>
  </div>
  
  <!-- Scroll Indicator -->
  <div class="scroll-indicator animate-fade-in" style="--delay: 1.5s">
    <div class="scroll-mouse">
      <div class="scroll-wheel"></div>
    </div>
    <span class="scroll-text">Scroll to explore</span>
  </div>
</section>

<!-- Hero Video Player Script -->
<script>
document.addEventListener('DOMContentLoaded', function() {
  const heroVideo = document.getElementById('heroVideo');
  const heroPoster = document.getElementById('heroPoster');
  const heroPlayBtn = document.getElementById('heroPlayBtn');
  const playBtnText = document.querySelector('.play-btn-text');
  
  if (heroPlayBtn && heroVideo && heroPoster) {
    let isPlaying = false;
    
    heroPlayBtn.addEventListener('click', function() {
      if (!isPlaying) {
        // Start playing video
        heroVideo.play();
        heroVideo.classList.add('playing');
        heroPoster.classList.add('hidden');
        heroPlayBtn.classList.add('playing');
        playBtnText.textContent = 'Pause Video';
        
        // Update icon to pause
        const icon = heroPlayBtn.querySelector('i');
        if (icon) {
          icon.setAttribute('data-lucide', 'pause');
          lucide.createIcons();
        }
        isPlaying = true;
      } else {
        // Pause video
        heroVideo.pause();
        heroVideo.classList.remove('playing');
        heroPoster.classList.remove('hidden');
        heroPlayBtn.classList.remove('playing');
        playBtnText.textContent = 'Watch Video';
        
        // Update icon to play
        const icon = heroPlayBtn.querySelector('i');
        if (icon) {
          icon.setAttribute('data-lucide', 'play');
          lucide.createIcons();
        }
        isPlaying = false;
      }
    });
  }
});
</script>

<style>
  /* Hero Section - Premium Clean Design */
  .hero-section-logistiqo {
    position: relative;
    min-height: 100vh;
    display: flex;
    align-items: center;
    overflow: hidden;
    background: #ffffff;
    padding: 16px;
  }

  /* Page Background */
  .hero-page-bg {
    position: absolute;
    inset: 0;
    background: #ffffff;
    z-index: 0;
  }

  /* Professional Edge Frame */
  .hero-edge-frame {
    position: absolute;
    top: 16px;
    left: 16px;
    right: 16px;
    bottom: 16px;
    z-index: 1;
  }

  /* Simple Corner Strokes - All 4 Corners */
  .corner-stroke {
    position: absolute;
    width: 60px;
    height: 60px;
    z-index: 10;
    pointer-events: none;
  }

  /* Top Left */
  .corner-tl {
    top: -4px;
    left: -4px;
    border-top: 2px solid #3B82F6;
    border-left: 2px solid #3B82F6;
    border-top-left-radius: 40px;
  }

  /* Top Right */
  .corner-tr {
    top: -4px;
    right: -4px;
    border-top: 2px solid #3B82F6;
    border-right: 2px solid #3B82F6;
    border-top-right-radius: 40px;
  }

  /* Bottom Left */
  .corner-bl {
    bottom: -4px;
    left: -4px;
    border-bottom: 2px solid #3B82F6;
    border-left: 2px solid #3B82F6;
    border-bottom-left-radius: 40px;
  }

  /* Bottom Right */
  .corner-br {
    bottom: -4px;
    right: -4px;
    border-bottom: 2px solid #3B82F6;
    border-right: 2px solid #3B82F6;
    border-bottom-right-radius: 40px;
  }

  /* Outer Edge - Premium Border Effect */
  .edge-outer {
    position: absolute;
    inset: 0;
    border-radius: 36px;
    background: #ffffff;
    padding: 6px;
    box-shadow: 
      0 40px 100px rgba(0, 0, 0, 0.15),
      0 20px 50px rgba(0, 0, 0, 0.1),
      0 8px 20px rgba(0, 0, 0, 0.06);
  }

  /* Secondary Edge - Middle Frame with Blue Accent */
  .edge-inner {
    position: absolute;
    top: 6px;
    left: 6px;
    right: 6px;
    bottom: 6px;
    border-radius: 30px;
    background: transparent;
    border: 2px solid rgba(59, 130, 246, 0.3);
    box-shadow: 
      inset 0 0 60px rgba(0, 0, 0, 0.05),
      0 0 20px rgba(59, 130, 246, 0.1);
    pointer-events: none;
    z-index: 3;
  }

  /* Floating Particles */
  .hero-particles {
    position: absolute;
    inset: 0;
    z-index: 10;
    pointer-events: none;
    overflow: hidden;
    border-radius: 30px;
  }

  .particle {
    position: absolute;
    width: 6px;
    height: 6px;
    background: linear-gradient(135deg, #3B82F6, #60A5FA);
    border-radius: 50%;
    opacity: 0.6;
    animation: floatUp 12s infinite ease-in-out;
  }

  .particle.p1 {
    left: 10%;
    bottom: -10px;
    animation-delay: 0s;
    animation-duration: 14s;
  }

  .particle.p2 {
    left: 30%;
    bottom: -10px;
    animation-delay: -3s;
    animation-duration: 12s;
    width: 8px;
    height: 8px;
  }

  .particle.p3 {
    left: 50%;
    bottom: -10px;
    animation-delay: -6s;
    animation-duration: 16s;
    width: 4px;
    height: 4px;
  }

  .particle.p4 {
    left: 70%;
    bottom: -10px;
    animation-delay: -9s;
    animation-duration: 13s;
  }

  .particle.p5 {
    left: 90%;
    bottom: -10px;
    animation-delay: -4s;
    animation-duration: 15s;
    width: 5px;
    height: 5px;
  }

  @keyframes floatUp {
    0% {
      transform: translateY(0) translateX(0) scale(1);
      opacity: 0;
    }
    10% {
      opacity: 0.6;
    }
    50% {
      transform: translateY(-50vh) translateX(20px) scale(1.2);
      opacity: 0.4;
    }
    90% {
      opacity: 0.2;
    }
    100% {
      transform: translateY(-100vh) translateX(-10px) scale(0.8);
      opacity: 0;
    }
  }

  /* Inset Image Container */
  .hero-inset-image {
    position: absolute;
    top: 6px;
    left: 6px;
    right: 6px;
    bottom: 6px;
    border-radius: 28px;
    overflow: hidden;
    z-index: 2;
  }

  .hero-bg-video {
    position: absolute;
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
    opacity: 0;
    transition: opacity 0.8s ease;
    z-index: 1;
  }

  .hero-bg-video.playing {
    opacity: 1;
  }

  .hero-bg-image {
    position: absolute;
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
    transition: opacity 0.8s ease, transform 8s ease-out;
    z-index: 2;
  }

  .hero-bg-image.hidden {
    opacity: 0;
  }

  .hero-section-logistiqo:hover .hero-bg-image:not(.hidden) {
    transform: scale(1.03);
  }

  .hero-image-overlay {
    position: absolute;
    inset: 0;
    background: 
      linear-gradient(90deg, rgba(0, 0, 0, 0.65) 0%, rgba(0, 0, 0, 0.4) 50%, rgba(0, 0, 0, 0.15) 80%, transparent 100%),
      linear-gradient(180deg, rgba(0, 0, 0, 0.3) 0%, transparent 30%, transparent 70%, rgba(0, 0, 0, 0.2) 100%);
    z-index: 4;
    pointer-events: none;
  }

  /* Play Video Button */
  .hero-play-btn {
    position: absolute;
    bottom: 40px;
    right: 40px;
    display: flex;
    align-items: center;
    gap: 14px;
    padding: 8px 24px 8px 8px;
    background: rgba(255, 255, 255, 0.15);
    backdrop-filter: blur(20px);
    -webkit-backdrop-filter: blur(20px);
    border: 1px solid rgba(255, 255, 255, 0.25);
    border-radius: 60px;
    cursor: pointer;
    z-index: 20;
    transition: all 0.4s ease;
  }

  .hero-play-btn:hover {
    background: rgba(255, 255, 255, 0.25);
    transform: scale(1.05);
    box-shadow: 0 10px 40px rgba(0, 0, 0, 0.2);
  }

  .hero-play-btn.playing {
    background: rgba(0, 0, 0, 0.5);
  }

  .play-btn-circle {
    width: 48px;
    height: 48px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #ffffff;
    border-radius: 50%;
    transition: all 0.3s ease;
  }

  .hero-play-btn:hover .play-btn-circle {
    transform: scale(1.1);
  }

  .play-btn-circle i {
    width: 20px;
    height: 20px;
    color: #1a1a2e;
    margin-left: 3px;
  }

  .hero-play-btn.playing .play-btn-circle i {
    margin-left: 0;
  }

  .play-btn-text {
    font-family: 'Inter', sans-serif;
    font-size: 14px;
    font-weight: 600;
    color: #ffffff;
    letter-spacing: 0.02em;
  }

  .hero-logistiqo-container {
    position: relative;
    z-index: 10;
    width: 100%;
    max-width: 100%;
    margin: 0;
    padding: 0 60px;
  }

  .hero-content-wrapper {
    display: flex;
    align-items: center;
    min-height: calc(100vh - 32px);
  }

  /* Left Text Content - Full Width */
  .hero-text-content {
    width: 100%;
    padding-top: 40px;
    padding-left: 20px;
  }

  /* Badge */
  .hero-badge {
    display: inline-flex;
    align-items: center;
    gap: 12px;
    margin-bottom: 40px;
    padding: 10px 20px 10px 14px;
    background: rgba(255, 255, 255, 0.1);
    backdrop-filter: blur(10px);
    -webkit-backdrop-filter: blur(10px);
    border: 1px solid rgba(255, 255, 255, 0.15);
    border-radius: 50px;
  }

  .badge-dot {
    width: 10px;
    height: 10px;
    background: #3B82F6;
    border-radius: 50%;
    animation: pulseDot 2s ease-in-out infinite;
    box-shadow: 0 0 12px rgba(59, 130, 246, 0.6);
  }

  @keyframes pulseDot {
    0%, 100% { opacity: 1; transform: scale(1); box-shadow: 0 0 12px rgba(59, 130, 246, 0.6); }
    50% { opacity: 0.8; transform: scale(1.2); box-shadow: 0 0 20px rgba(59, 130, 246, 0.8); }
  }

  .badge-label {
    font-family: 'Inter', sans-serif;
    font-size: 13px;
    font-weight: 500;
    color: rgba(255, 255, 255, 0.9);
    letter-spacing: 0.04em;
    text-transform: uppercase;
  }

  /* Headline - Full Width Single Lines */
  .hero-headline {
    font-family: 'Space Grotesk', sans-serif;
    font-weight: 700;
    line-height: 1.05;
    letter-spacing: -0.03em;
    margin-bottom: 36px;
  }

  .headline-line {
