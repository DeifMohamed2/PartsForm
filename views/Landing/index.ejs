  /* Animations */
  .animate-slide-up {
    opacity: 0;
    transform: translateY(50px);
    animation: slideUp 1s cubic-bezier(0.4, 0, 0.2, 1) forwards;
    animation-delay: var(--delay, 0s);
  }

  @keyframes slideUp {
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }

  /* Slide from Left Animation - Smooth & Faster */
  .animate-slide-left {
    opacity: 0;
    transform: translateX(-50px);
    animation: slideFromLeft 1.4s cubic-bezier(0.25, 0.46, 0.45, 0.94) forwards;
    animation-delay: var(--delay, 0s);
  }

  @keyframes slideFromLeft {
    0% {
      opacity: 0;
      transform: translateX(-50px);
    }
    50% {
      opacity: 0.7;
    }
    100% {
      opacity: 1;
      transform: translateX(0);
    }
  }

