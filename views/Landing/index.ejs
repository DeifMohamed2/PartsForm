    max-width: 700px;
    text-shadow: 0 2px 15px rgba(0, 0, 0, 0.3);
  }

  /* CTA Button - Premium Design */
  .hero-cta-group {
    display: flex;
    align-items: center;
  }

  .btn-cta-premium {
    position: relative;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    padding: 20px 44px;
    font-family: 'Inter', sans-serif;
    font-size: 16px;
    font-weight: 600;
    color: #ffffff;
    background: linear-gradient(135deg, #2B5278 0%, #3B82F6 50%, #2563EB 100%);
    background-size: 200% 200%;
    border: none;
    border-radius: 60px;
    cursor: pointer;
    text-decoration: none;
    transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
