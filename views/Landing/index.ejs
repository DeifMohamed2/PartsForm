<script>
  // About Premium Section - Scroll Animation Observer
  (function() {
    const aboutBanner = document.querySelector('.about-premium-container[data-about-animate]');
    if (!aboutBanner) return;

    const observer = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          aboutBanner.classList.add('animate-in');
          observer.unobserve(entry.target);
        }
      });
    }, {
      threshold: 0.15,
      rootMargin: '0px 0px -80px 0px'
    });

    observer.observe(aboutBanner);

    // Fallback for page load
    window.addEventListener('load', () => {
      const rect = aboutBanner.getBoundingClientRect();
      if (rect.top < window.innerHeight && rect.bottom >= 0) {
        setTimeout(() => {
          aboutBanner.classList.add('animate-in');
        }, 400);
      }
    });
  })();
</script>

