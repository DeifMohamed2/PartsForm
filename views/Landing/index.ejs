<!-- ====================================
         SCRIPTS
         ==================================== -->
<script src="/js/animations.js"></script>
<script src="/js/main.js"></script>
<script>
  // Initialize Lucide icons
  lucide.createIcons();

  // Hero Background Slider (only if slides exist)
  const slides = document.querySelectorAll('.hero-bg-slide');
  if (slides.length > 0) {
    let currentSlide = 0;
  const totalSlides = slides.length;

  function changeSlide() {
    slides[currentSlide].classList.remove('active');
    currentSlide = (currentSlide + 1) % totalSlides;
    slides[currentSlide].classList.add('active');
  }

  // Change slide every 5 seconds
  setInterval(changeSlide, 5000);
  }

  // Industry Cards Intersection Observer for Professional Entrance
  const observeIndustryCards = () => {
    const industryCards = document.querySelectorAll('.industry-card[data-animate="industry-entrance"]');

    const observer = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          entry.target.style.animationPlayState = 'running';
          observer.unobserve(entry.target);
        }
      });
    }, {
      threshold: 0.15,
      rootMargin: '0px 0px -50px 0px'
    });

    industryCards.forEach(card => {
      card.style.animationPlayState = 'paused';
      observer.observe(card);
    });
  };

  // Subtle Parallax Effect on Industry Card Images
  const addParallaxEffect = () => {
    const industryCards = document.querySelectorAll('.industry-card');

    industryCards.forEach(card => {
      card.addEventListener('mousemove', (e) => {
        const rect = card.getBoundingClientRect();
        const x = e.clientX - rect.left;
        const y = e.clientY - rect.top;

        const centerX = rect.width / 2;
        const centerY = rect.height / 2;

        const deltaX = (x - centerX) / centerX;
        const deltaY = (y - centerY) / centerY;

        const image = card.querySelector('.industry-card-image img');
        const icon = card.querySelector('.industry-icon');

        if (image) {
          image.style.transform = `scale(1.15) translate(${deltaX * 10}px, ${deltaY * 10}px)`;
        }

        if (icon) {
          icon.style.transform = `scale(1.1) rotate(-5deg) translate(${deltaX * 5}px, ${deltaY * 5}px)`;
        }
      });

      card.addEventListener('mouseleave', () => {
        const image = card.querySelector('.industry-card-image img');
        const icon = card.querySelector('.industry-icon');

        if (image) {
          image.style.transform = '';
        }

        if (icon) {
          icon.style.transform = '';
        }
      });
    });
  };

  // Contact Section Scroll Animations
  const observeContactSection = () => {
    const contactSection = document.querySelector('.contact-section[data-contact-section]');
    if (!contactSection) return;

    const observer = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          entry.target.classList.add('in-view');
          observer.unobserve(entry.target);
        }
      });
    }, {
      threshold: 0.15,
      rootMargin: '0px 0px -80px 0px'
    });

    observer.observe(contactSection);
  };

  // Source Globally Section Intersection Observer
  const observeSourceGloballySection = () => {
    const section = document.querySelector('.source-globally-section');
    if (!section) return;

    const observer = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          entry.target.classList.add('in-view');
          observer.unobserve(entry.target);
        }
      });
    }, {
      threshold: 0.15,
      rootMargin: '0px 0px -80px 0px'
    });

    observer.observe(section);
  };

  // Run observer immediately and on load
  observeSourceGloballySection();
  observeContactSection();

  // Observe Advantages Timeline Section - handled by inline script
  // Legacy observer removed - using new timeline animation system

  // Counter animation function
  const animateCounter = (stat, target, statType) => {
    const start = 0;
    const duration = 2000;
    const startTime = performance.now();

    const updateCounter = (currentTime) => {
      const elapsed = currentTime - startTime;
      const progress = Math.min(elapsed / duration, 1);

      // Ease out cubic
      const easeProgress = 1 - Math.pow(1 - progress, 3);
      const current = start + (target - start) * easeProgress;

      if (statType === 'billion') {
        stat.textContent = current.toFixed(1);
      } else if (statType === 'percentage') {
        stat.textContent = current.toFixed(1);
      } else if (statType === 'number') {
        stat.textContent = Math.floor(current).toLocaleString();
      } else {
        stat.textContent = Math.floor(current).toLocaleString();
      }

      if (progress < 1) {
        requestAnimationFrame(updateCounter);
      } else {
        // Ensure final value is exact
        if (statType === 'billion') {
          stat.textContent = target.toFixed(1);
        } else if (statType === 'percentage') {
          stat.textContent = target.toFixed(1);
        } else if (statType === 'number') {
          stat.textContent = Math.floor(target).toLocaleString();
        } else {
          stat.textContent = Math.floor(target).toLocaleString();
        }
      }
    };

    requestAnimationFrame(updateCounter);
  };

  // Start counter animations on page load
  function startCounterAnimations() {
    const statNumbers = document.querySelectorAll('.stat-number[data-count]');
    statNumbers.forEach((stat, index) => {
      // Reset to 0 first
      stat.textContent = '0';

      const target = parseFloat(stat.dataset.count);
      const statType = stat.dataset.type || 'number';

      // Start animation with delay
      setTimeout(() => {
        animateCounter(stat, target, statType);
      }, 800 + (index * 200));
    });
  }

  // Reinitialize icons after dynamic content loads
  window.addEventListener('load', () => {
    lucide.createIcons();
    // observeIndustryCards(); // Disabled - removed scroll animation
    observeSourceGloballySection();
    observeContactSection();
    addParallaxEffect();

    // Start counter animations
    startCounterAnimations();
  });

  // Also trigger on DOMContentLoaded as fallback
  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', startCounterAnimations);
  } else {
    startCounterAnimations();
  }

  // ====================================
  // LOGIN ROUTING - Admin vs Buyer
  // ====================================
  const signInForm = document.getElementById('sign-in-form');
  if (signInForm) {
    signInForm.addEventListener('submit', function(e) {
      e.preventDefault();
      
      const emailInput = signInForm.querySelector('input[type="email"]');
      const passwordInput = signInForm.querySelector('input[type="password"]');
      const submitBtn = signInForm.querySelector('button[type="submit"]');
      
      const email = emailInput.value.trim().toLowerCase();
      const password = passwordInput.value;
      
      // Admin credentials
      const ADMIN_EMAIL = 'admin@partsform.com';
      const ADMIN_PASSWORD = 'admin123';
      
      // Buyer accounts
      const BUYER_ACCOUNTS = [
        { email: 'buyer@partsform.com', password: 'buyer123', name: 'Demo Buyer' },
        { email: 'john.smith@automax.com', password: 'buyer123', name: 'John Smith' },
        { email: 'maria@premiumauto.com', password: 'buyer123', name: 'Maria Garcia' },
        { email: 'sarah@partsworld.com', password: 'buyer123', name: 'Sarah Johnson' },
        { email: 'robert@euroauto.eu', password: 'buyer123', name: 'Robert Chen' }
      ];
      
      // Show loading state
      const originalBtnContent = submitBtn.innerHTML;
      submitBtn.innerHTML = '<span>Signing in...</span><i data-lucide="loader" class="spin"></i>';
      submitBtn.disabled = true;
      lucide.createIcons();
      
      // Simulate authentication delay
      setTimeout(() => {
        // Check if admin credentials
        if (email === ADMIN_EMAIL && password === ADMIN_PASSWORD) {
          // Store admin session
          localStorage.setItem('userRole', 'admin');
          localStorage.setItem('userName', 'Admin User');
          localStorage.setItem('userEmail', email);
          localStorage.setItem('isLoggedIn', 'true');
          
          // Redirect to admin dashboard
          window.location.href = '/admin';
        } else {
          // Check if buyer credentials
          const buyer = BUYER_ACCOUNTS.find(b => b.email === email && b.password === password);
          
          if (buyer) {
            // Store buyer session
            localStorage.setItem('userRole', 'buyer');
            localStorage.setItem('userName', buyer.name);
            localStorage.setItem('userEmail', email);
            localStorage.setItem('isLoggedIn', 'true');
            
            // Redirect to buyer portal
            window.location.href = '/buyer';
          } else {
            // Reset button on error
            submitBtn.innerHTML = originalBtnContent;
            submitBtn.disabled = false;
            lucide.createIcons();
            alert('Invalid email or password. Please try again.');
          }
        }
      }, 1000);
    });
  }

  // Add spinner animation style
  const style = document.createElement('style');
  style.textContent = `
    .spin {
      animation: spin 1s linear infinite;
    }
    @keyframes spin {
      from { transform: rotate(0deg); }
      to { transform: rotate(360deg); }
    }
  `;
  document.head.appendChild(style);
</script>
