<script>
  // Timeline Section - Individual Element Animation Observer
  // Elements animate once when scrolling down, stay visible when scrolling back
  (function() {
    const timelineSection = document.querySelector('.advantages-timeline-section');
    if (!timelineSection) return;

    const animatedElements = timelineSection.querySelectorAll('[data-timeline-animate]');
    
    // Individual element observer - animate once only
    const elementObserver = new IntersectionObserver((entries, observer) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          // Element is in view - animate in
          entry.target.classList.add('visible');
          // Stop observing this element - animation only happens once
          observer.unobserve(entry.target);
        }
      });
    }, {
      threshold: 0.2,
      rootMargin: '-50px 0px -50px 0px'
    });

    // Observe each animated element individually
    animatedElements.forEach(element => {
      elementObserver.observe(element);
    });

    // Line Progress on Scroll
    const updateLineProgress = () => {
      const rect = timelineSection.getBoundingClientRect();
      const lineProgress = timelineSection.querySelector('.line-progress');
      if (!lineProgress) return;

      const windowHeight = window.innerHeight;
      const sectionTop = rect.top;
      const sectionHeight = rect.height;

      if (sectionTop < windowHeight && rect.bottom > 0) {
        const progress = Math.min(100, Math.max(0, 
          ((windowHeight - sectionTop) / (sectionHeight + windowHeight * 0.3)) * 100
        ));
        lineProgress.style.height = progress + '%';
      } else if (rect.bottom < 0) {
        lineProgress.style.height = '100%';
      } else {
        lineProgress.style.height = '0%';
      }
    };

    window.addEventListener('scroll', updateLineProgress, { passive: true });
    updateLineProgress();
  })();
</script>

